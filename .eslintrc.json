{
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"extends": "airbnb",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"parser": "babel-eslint",
	"plugins": ["react"],
	"rules": {
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"react/prefer-stateless-function": [0],
		"react/jsx-indent": [1, "tab"],
		"react/jsx-indent-props": [1, "tab"],
		"react/no-unused-state": [0],
		"react/no-mixed-spaces-and-tabs": [0],
		"react/destructuring-assignment": [0],
		"no-tabs": [0],
		"no-mixed-spaces-and-tabs": [0],
		"no-return-assign": [0],
		"no-undef": [0],
		"indent": [1, "tab"],
		"import/prefer-default-export": [0],
		"no-unused-vars": [1],
		"arrow-parens": [0],
		"react/forbid-prop-types": [0],
		"no-plusplus": [0],
		"linebreak-style": [0]
	}
}
